{
    "name": "jonasgn/laravel-commons",
    "description": "Possui métodos auxiliares que são comuns em toda aplicação laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jonasgn\\LaravelCommons\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jonas G Neto",
            "email": "j.goncalvesneto@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.2",
        "pestphp/pest": "^3.8"
    }
}
